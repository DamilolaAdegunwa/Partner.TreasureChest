@using KnockoutJS.Web.ViewModels
@model IEnumerable<BookViewModel>
@{
    Layout = "~/Views/Shared/_LayoutScreen.cshtml";
    ViewData["Title"] = "Books";
}

<h2>@ViewData["Title"]</h2>
@if (Model.Count() > 0)
{
    @await Html.PartialAsync("_List", Model)
}
else
{
    <div class="alert alert-info">
        当前分类下没有书籍
    </div>
}

